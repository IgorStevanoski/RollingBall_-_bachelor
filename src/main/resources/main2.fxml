<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.Cursor?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.FlowPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="800.0" prefWidth="1280.0" style="-fx-background-color: transaprent;" xmlns="http://javafx.com/javafx/20.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.rollingBall.MainMenuController">
   <cursor>
      <Cursor fx:constant="DEFAULT" />
   </cursor>
   <children>
      <FlowPane alignment="CENTER" layoutX="565.0" prefHeight="800.0" prefWidth="150.0" vgap="50.0">
         <children>
            <Label alignment="CENTER" contentDisplay="CENTER" prefHeight="58.0" prefWidth="150.0" text="RollingBall" textFill="WHITE" textOverrun="CLIP">
               <font>
                  <Font name="Comic Sans MS" size="31.0" />
               </font>
            </Label>
            <Button fx:id="newGameButton" mnemonicParsing="false" onAction="#newGame" onMouseEntered="#onHoverNewGame" prefHeight="20.0" prefWidth="151.0" text="New Game" />
            <Button fx:id="continueButton" mnemonicParsing="false" onMouseEntered="#onHoverNewGame" prefHeight="25.0" prefWidth="151.0" text="Continue" />
            <Button fx:id="playerButton" mnemonicParsing="false" onMouseEntered="#onHoverNewGame" prefHeight="25.0" prefWidth="151.0" text="Player" />
            <Button fx:id="leaderBoardsButton" mnemonicParsing="false" onMouseEntered="#onHoverNewGame" prefHeight="25.0" prefWidth="151.0" text="Leaderboards" />
            <Button fx:id="settingsButton" mnemonicParsing="false" onMouseEntered="#onHoverNewGame" prefHeight="25.0" prefWidth="151.0" text="Settings" />
            <Button fx:id="editorButton" mnemonicParsing="false" onMouseEntered="#onHoverNewGame" prefHeight="25.0" prefWidth="151.0" text="Editor" />
            <Button fx:id="commandsButton" mnemonicParsing="false" onMouseEntered="#onHoverNewGame" prefHeight="25.0" prefWidth="151.0" text="Commands" />
            <Button fx:id="aboutButton" mnemonicParsing="false" onMouseEntered="#onHoverNewGame" prefHeight="25.0" prefWidth="151.0" text="About" />
            <Button fx:id="quitButton" mnemonicParsing="false" onAction="#quit" onMouseEntered="#onHoverNewGame" prefHeight="25.0" prefWidth="151.0" text="Quit" />
         </children>
      </FlowPane>
   </children>
</AnchorPane>
