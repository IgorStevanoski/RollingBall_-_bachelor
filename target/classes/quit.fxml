<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.Cursor?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.FlowPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="800.0" prefWidth="1280.0" style="-fx-background-color: transparent;" xmlns="http://javafx.com/javafx/20.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.rollingBall.QuitMenuController">
   <cursor>
      <Cursor fx:constant="DEFAULT" />
   </cursor>
   <children>
      <FlowPane alignment="CENTER" layoutX="565.0" prefHeight="800.0" prefWidth="151.0" vgap="50.0">
         <children>
            <Button fx:id="yesButton" mnemonicParsing="false" onAction="#quit" prefHeight="20.0" prefWidth="151.0" text="Yes" />
            <Button fx:id="noButton" mnemonicParsing="false" prefHeight="25.0" prefWidth="151.0" text="No" />
         </children>
      </FlowPane>
      <Label alignment="CENTER" contentDisplay="CENTER" layoutX="-14.0" layoutY="234.0" prefHeight="58.0" prefWidth="1280.0" text="Are you sure you want to quit?" textFill="WHITE" textOverrun="CLIP">
         <font>
            <Font name="Comic Sans MS" size="31.0" />
         </font>
      </Label>
   </children>
</AnchorPane>
